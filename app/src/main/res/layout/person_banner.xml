<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
xmlns:app="http://schemas.android.com/apk/res-auto"
android:layout_width="match_parent"
android:layout_height="match_parent">

<EditText
    android:id="@+id/bannerPersonMail"
    android:layout_width="172dp"
    android:layout_height="39dp"
    android:layout_above="@+id/bannerText2"
    android:layout_alignParentStart="true"
    android:layout_marginStart="0dp"
    android:layout_marginEnd="0dp"
    android:layout_marginBottom="0dp"
    android:layout_toStartOf="@+id/bannerPhoto"
    android:background="@drawable/back"
    android:ems="10"
    android:inputType="textPersonName"
    android:text=""
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/BannerPersonName"
    tools:ignore="LabelFor,UnknownId" />
<EditText
    android:id="@+id/bannerPersonName"
    android:layout_width="wrap_content"
    android:layout_height="43dp"
    android:layout_above="@+id/bannerPersonMail"
    android:layout_alignParentStart="true"
    android:layout_alignParentTop="true"
    android:layout_marginStart="0dp"
    android:layout_marginTop="0dp"
    android:layout_marginEnd="0dp"
    android:layout_marginBottom="0dp"
    android:layout_toStartOf="@+id/bannerPhoto"
    android:background="@drawable/back"
    android:ems="10"
    android:inputType="textPersonName"
    android:text=""
    app:layout_constraintEnd_toStartOf="@+id/BannerPhoto"
    app:layout_constraintHorizontal_bias="1.0"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    tools:ignore="LabelFor" />

    <TextView
        android:id="@+id/bannerText2"
        android:layout_width="170dp"
        android:layout_height="wrap_content"
        android:layout_above="@+id/BannerListOfInterests1"
        android:layout_alignParentStart="true"
        android:layout_marginStart="0dp"
        android:layout_marginEnd="0dp"
        android:layout_marginBottom="0dp"
        android:layout_toStartOf="@+id/bannerPhoto"
        android:gravity="center"
        android:text="@string/interests"
        app:layout_constraintBottom_toTopOf="@+id/BannerListOfInterests1"
        app:layout_constraintStart_toStartOf="parent" />

<ListView
    android:id="@+id/BannerListOfInterests1"
    android:layout_width="166dp"
    android:layout_height="307dp"
    android:layout_above="@+id/SkipButton"
    android:layout_alignParentStart="true"
    android:layout_marginStart="0dp"
    android:layout_marginEnd="0dp"
    android:layout_marginBottom="0dp"
    android:layout_toStartOf="@+id/bannerPhoto"
    android:background="@drawable/back"
    app:layout_constraintBottom_toBottomOf="@+id/BannerPhoto"
    app:layout_constraintEnd_toStartOf="@+id/BannerPhoto"
    app:layout_constraintHorizontal_bias="0.0"
    app:layout_constraintStart_toStartOf="parent"
    tools:ignore="UnknownId" />

<ImageView
    android:id="@+id/bannerPhoto"
    android:layout_width="237dp"
    android:layout_height="415dp"
    android:layout_above="@+id/SkipButton"
    android:layout_alignParentTop="true"
    android:layout_alignParentEnd="true"
    android:layout_marginTop="0dp"
    android:layout_marginEnd="0dp"
    android:layout_marginBottom="0dp"
    android:background="@drawable/back"
    android:contentDescription=""
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    tools:srcCompat="@tools:sample/avatars" />
<Button
    android:id="@+id/SkipButton"
    android:layout_width="205dp"
    android:layout_height="wrap_content"
    android:layout_below="@+id/bannerAboutMe"
    android:layout_alignParentEnd="true"
    android:layout_alignParentBottom="true"
    android:layout_marginTop="0dp"
    android:layout_marginEnd="-1dp"
    android:layout_marginBottom="126dp"
    android:text="Like"
    tools:ignore="UnknownId" />

<Button
    android:id="@+id/SkipButton"
    android:layout_width="205dp"
    android:layout_height="wrap_content"
    android:layout_alignParentStart="true"
    android:layout_alignParentBottom="true"
    android:layout_marginStart="0dp"
    android:layout_marginEnd="0dp"
    android:layout_marginBottom="125dp"
    android:layout_toStartOf="@+id/SkipButton"
    android:text="Skip" />
</RelativeLayout>